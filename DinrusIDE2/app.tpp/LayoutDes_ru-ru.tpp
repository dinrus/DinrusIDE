[s0;l321; [C@5;1 LAYOUT(ConfigLayout, 724, 214)]&]
[s7; -|ITEM(Upp`::ArrayCtrl, list, AutoHideSb(true).HSizePosZ(4, 4).VSizePosZ(6, 
32))&]
[s7; -|ITEM(Upp`::Button, ok, SetLabel(t`_(`"OK`")).RightPosZ(88, 80).BottomPosZ(4, 
22))&]
[s7; -|ITEM(Upp`::Button, cancel, SetLabel(t`_(`"Отмена`")).RightPosZ(4, 
80).BottomPosZ(4, 22))&]
[s7; END`_LAYOUT&]
